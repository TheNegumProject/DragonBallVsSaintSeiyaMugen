trigger1		= (((StateType != A) && (Ctrl)) || ((StateType = A) && (Ctrl) && (Var(20) = 1))) && (Command != "holdfwd") && (Command != "holdback")
trigger2		= (Var(3) = 1) || ((Var(3) = 3) && (PrevStateNo != 8040)) || ((StateNo = [150,152]) && (Power >= 500))

